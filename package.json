{
  "name": "playwright-step-framework",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:old": "playwright test --config=playwright.old.config.js",
    "test:new": "playwright test --config=playwright.new.config.ts",
    "report:old": "playwright show-report --config=playwright.old.config.js --port=9340",
    "report:new": "playwright show-report --config=playwright.new.config.ts --port=9340",
    "lint": "echo \"add eslint if needed\""
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "typescript": "^5.6.2"
  }
}
